#include <iostream>
using namespace std;

int main()
{
    // Declare variables
    int x;
    int y;
    int total;

    // Set values 
    x = 50;
    y = 100;
    
    // calculate total
    total = x + y;

    cout << "The sum of " << x << " and " << y << " is: " << total << endl;

    return 0;
}